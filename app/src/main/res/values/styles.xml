<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="back_button">
        <item name="android:layout_width">38dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:layout_marginStart">18dp</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="iconTint">@color/black</item>
        <item name="android:background">@drawable/shape_backbutton</item>
        <item name="android:paddingLeft">11dp</item>
    </style>

    <style name="inputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">@color/text_input_box_stroke</item>
        <item name="boxStrokeWidth">1dp</item>
    </style>

    <style name="ImageViewCirclee" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">65dp</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="ThemeCustomDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@null</item>
    </style>

    <style name="ImageViewCrop" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="BottonSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheetStyle</item>
    </style>

    <style name="BottomSheetStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>
</resources>